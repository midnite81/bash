# Please note that some of these aliases use
# added on git commands

alias gs='git status'
alias ga='git add $*'
alias gaa='git add -A'
alias gc='git commit -m '
alias gpo='git push origin $1'
alias gcc='git checkout $1'
alias gm='git merge $1'
alias gf='git fetch --all'
alias gi='git ilist'

gac() {
  git add -A
  git commit -m "$*"
}

gacp() {
  git add -A
  git commit -m "$*"
  git pushc
}